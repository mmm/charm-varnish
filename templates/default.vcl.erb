# Generated by juju

<% active_backends.each do |server_name, server_info| %>
backend <%= server_name %> { 
  .host = "<%= server_info[:address] %>";
  .port = "<%= server_info[:port] %>";
}
<% end %>

director baz round-robin {
  <% active_backends.keys.each do |server_name| %>
  {
    .backend = <%= server_name %>;
  }
  <% end %>
}

sub vcl_recv {
  set req.backend = baz;
}

