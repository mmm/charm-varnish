<% current_backends.keys.each do |server| %>
backend <%= server %> {
  .host = "<%= current_backends[server][:address] %>";
  .port = "<%= current_backends[server][:port] %>";
}
<% end %>

director baz round-robin {
  <% current_backends.keys.each do |server| %>
  {
    .backend = <%= server %>;
  }
  <% end %>
}

sub vcl_recv {
  set req.backend = baz;
}

